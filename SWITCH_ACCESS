!#######  Switch d'Acc√®s    ######
!#################################

enable
  configure terminal
    hostname SW-A1
    no ip domain lookup
		ip domain name Smart.Farming.LAB
		enable secret Smart
		service password-encryption
		crypto key generate rsa modulus 1024
		username Talion privilege 15 secret Vodka
		ip ssh version 2
    !ip default-gateway xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    banner motd # Unauthorized access to this device is prohibited! #
    
    line con 0 
			logging synchronous
      password Vodka
			exit

		line vty 0 15
			logging synchronous
			login local
			transport input ssh
			exit

		interface range Fa0/1-24
			shutdown
      no switchport mode
			switchport nonegotiate
			exit
		interface range Gigabitethernet0/1-2
			shutdown
      no switchport mode
			switchport nonegotiate
			exit
